{
  "name": "@high-u/mcp-server-bridge-restish",
  "version": "0.1.23",
  "description": "Bridge Restish MCP Server",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-server-bridge-restish": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "node --experimental-strip-types --watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node --format=esm --external:effect --external:@valibot/to-json-schema --external:path --external:fs --external:http --external:https --external:url --external:crypto --external:stream --external:util --external:events --external:os --external:buffer --external:string_decoder --external:zlib --external:async_hooks --external:http2 --external:diagnostics_channel",
    "prepare": "npm run build",
    "start": "node dist/index.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "fastmcp": "^1.27.6",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "esbuild": "^0.25.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/high-u/mcp-server-bridge-restish.git"
  },
  "keywords": [
    "mcp",
    "bridge",
    "restish",
    "rest",
    "api"
  ],
  "author": "high-u",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/high-u/mcp-server-bridge-restish/issues"
  }
}
